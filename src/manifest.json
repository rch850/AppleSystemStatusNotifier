{
  "name": "AppleSystemStatus",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Display Apple System Status.",
  "icons": {"128": "icon128.png"},
  "background": {
    "scripts": ["jquery-1.8.3.min.js", "eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "permissions": [
    "alarms", "http://www.apple.com/"
  ]
}
