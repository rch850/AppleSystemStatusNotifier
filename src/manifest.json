{
  "name": "Apple System Status Notifier",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Notify Apple System Status (unofficially).",
  "icons": {"128": "icon128.png"},
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms", "storage", "http://www.apple.com/"
  ]
}
